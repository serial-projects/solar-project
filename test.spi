say_yes:
    move    "YES!", !r0
    sysc    "write"
    halt
say_no:
    move    "NO!", !r0
    sysc    "write"
    halt
main:
    define  should_jump, false
    move    true,   !r0
    move    false,  !r1
    and     !r0, !r1, $should_jump
    move    $should_jump, !EQ
    je      say_yes
    jne     say_no